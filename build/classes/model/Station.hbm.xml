<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="model.Station" table="Station">
		<id name="stationId" type="int" column="stationId">
			<generator class="native"></generator>
		</id>
		<property name="stationName" type="string" column="stationName"></property>
	</class>
</hibernate-mapping>